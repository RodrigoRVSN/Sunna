{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": 0,
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
    "import/prefer-default-export": 0,
    "react/jsx-props-no-spreading": 0,
    "import/no-unresolved": 0,
    "no-console": 0,
    "no-alert": 0,
    "global-require": 0
  }
}
