{
  "expo": {
    "name": "Sunna",
    "slug": "Sunna",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "sunna",
    
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D1321"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rodrigorvsn.app"
    },
    "android": {
      "package": "com.rodrigorvsn.app",
      "googleServicesFile": "./google-services.json",
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D1321"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "UPXFour",
    "githubUrl": "https://github.com/RodrigoRVSN/upx_app/tree/appBranch", 
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/path/to/myNotificationIcon.png",
          "color": "#ffffff",
          "sounds": ["./local/path/to/mySound.wav", "./local/path/to/myOtherSound.wav"],
          "mode": "production"
        }
      ]
    ]
  
  }
}
